{
  "name": "travel-site",
  "version": "1.0.0",
  "description": "study site",
  "main": "gulpfile.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss app/assets/styles/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/app.comp.css",
    "concat:css": "concat -o css/app.concat.css css/app.comp.css css/icon-font.css",
    "prefix:css": "postcss --use autoprefixer -b 'browserslist' css/app.concat.css -o css/app.prefix.css",
    "compress:css": "node-sass css/app.prefix.css css/app.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ramiashqar/travel-site.git"
  },
  "author": "Rami Ashqar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ramiashqar/travel-site/issues"
  },
  "homepage": "https://github.com/Ramiashqar/travel-site#readme",
  "dependencies": {
    "jquery": "^3.3.1",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "browser-sync": "^2.23.6",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-svg-sprite": "^1.4.0",
    "gulp-watch": "^5.0.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "postcss-import": "^11.1.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0"
  }
}
